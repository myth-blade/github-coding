//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^coding-school.tech[nc]
RewriteRule ^(.*)$ http://www.coding-school.tech/$1 [r=301,nc]

//Password Protect file
<Files /roles.html>
AuthName "Prompt"
AuthType Basic
AuthUserFile /utils/.htpasswd
Require valid-user
</Files>

//Prevent viewing of .htaccess file
<Files .htaccess>
order allow,deny
deny from all
</Files>

//Prevent directory listings
Options All -Indexes
 